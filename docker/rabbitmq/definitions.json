{
  "rabbit_version": "3.12.0",
  "rabbitmq_version": "3.12.0",
  "product_name": "RabbitMQ",
  "users": [
    {
      "name": "rabbitmq",
      "password_hash": "JcF8eZ8sMfLsGvhbHcMW8xG8xqmLhLGj0qhNmPXvL7M=",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": ["administrator"]
    }
  ],
  "vhosts": [
    {
      "name": "/"
    },
    {
      "name": "/app"
    },
    {
      "name": "/tasks"
    }
  ],
  "permissions": [
    {
      "user": "rabbitmq",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    },
    {
      "user": "rabbitmq",
      "vhost": "/app",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    },
    {
      "user": "rabbitmq",
      "vhost": "/tasks",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "ai-infra-cluster"
    }
  ],
  "policies": [
    {
      "vhost": "/",
      "name": "ha-all",
      "pattern": ".*",
      "apply-to": "all",
      "definition": {
        "ha-mode": "all",
        "ha-sync-mode": "automatic"
      },
      "priority": 0
    },
    {
      "vhost": "/app",
      "name": "message-ttl",
      "pattern": "^task\\..*",
      "apply-to": "queues",
      "definition": {
        "message-ttl": 86400000,
        "max-length": 10000,
        "overflow": "reject-publish"
      },
      "priority": 1
    }
  ],
  "queues": [
    {
      "name": "task.default",
      "vhost": "/app",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic"
      }
    },
    {
      "name": "task.priority",
      "vhost": "/app",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-max-priority": 10,
        "x-queue-type": "classic"
      }
    },
    {
      "name": "notifications",
      "vhost": "/app",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "exchanges": [
    {
      "name": "task.exchange",
      "vhost": "/app",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "events.exchange",
      "vhost": "/app",
      "type": "fanout",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "task.exchange",
      "vhost": "/app",
      "destination": "task.default",
      "destination_type": "queue",
      "routing_key": "task.default",
      "arguments": {}
    },
    {
      "source": "task.exchange",
      "vhost": "/app",
      "destination": "task.priority",
      "destination_type": "queue",
      "routing_key": "task.priority",
      "arguments": {}
    }
  ]
}

